        <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="Set Dynatrace Headers" enabled="true">
          <boolProp name="resetInterpreter">true</boolProp>
          <stringProp name="parameters">CDelivery_Demo.JMeterTests.</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="script">import org.apache.jmeter.util.JMeterUtils;
import org.apache.jmeter.protocol.http.control.HeaderManager;
import java.io;
import java.util;

// -------------------------------------------------------------------------------------
// Will set the X-Dynatrace Header. Will look for DT_TESTRUN_ID (property) or alternativeyl looks for dttestrun.txt file including TESTRUNID
// ------------------------------------
// INPUT Parameters
// (optional) NAPREFIX: Prefix for NA X-Dynatrace Timer Name
// INPUT Properties
// (optional) DT_TESTRUN_ID: If DT_TESTRUN_ID is present will use it for TR Parameter 

// ------------------------------------------
// do we run as part of a Test Automation Build? check for Property DT_TESTRUN_ID
vars.put(&quot;testrunId&quot;, &quot;${__P(testrunID)}&quot;);
//String trid=JMeterUtils.getProperty(&quot;DT_TESTRUN_ID&quot;);
String trid=vars.get(&quot;testrunId&quot;);
if((trid == null) || (trid.length() == 0)) {
  // if we dont have it in the property - check if file dttestrun.txt exists!
  File f = new File(&quot;dttestrun.txt&quot;);
  if(f.exists()) {
  	print(&quot;dttestrun.txt exists!&quot;);
  	Scanner scan = new Scanner(f);
  	if(scan.hasNextLine()) trid = scan.nextLine();
  	scan.close();

     // setting it to the propoerty so that we dont have to read the file for every request
     JMeterUtils.setProperty(&quot;DT_TESTRUN_ID&quot;, trid);
  	print(&quot;DT_TESTRUN_ID from dttestrun.txt: &quot; + trid);
 }
}

// -------------------------------------------
// Generate regular dynatrace http header
// check if there is a TimerName Prefix coming in as parameter
String NAPREFIX = (bsh.args.length &gt; 0) ? bsh.args[0] : &quot;&quot;;
String NA = NAPREFIX + sampler.getName();
String VU = ctx.getThreadGroup().getName() + ctx.getThreadNum();
String TA = trid==null ? &quot;&quot; : &quot;;TR=&quot;+trid+&quot;;RC=200&quot;;
String headerValue = &quot;NA=&quot;+ NA + &quot;;VU=&quot; + VU + TA;

// want to test how this looks in Test Automation? - add TN=Your Timer Name
headerValue += &quot;;TN=&quot; + NAPREFIX + sampler.getName();

// -------------------------------------------
// log it out &amp; set header
print(&quot;Setting Dynatrace Headers: &quot; + headerValue);
HeaderManager hm = sampler.getHeaderManager();
hm.removeHeaderNamed(&quot;X-Dynatrace&quot;);
hm.add(new org.apache.jmeter.protocol.http.control.Header(&quot;X-Dynatrace&quot;, headerValue));</stringProp>
        </BeanShellPreProcessor>
